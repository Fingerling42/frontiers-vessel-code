<launch>
  <node name="trader" pkg="drone_on_volga" type="trader_node" output="screen">
    <!-- Address of a lighthouse you work on, airalab on sidechain lighthouse by default -->
    <param name="lighthouse" value="0xD40AC7F1e5401e03D00F5aeC1779D8e5Af4CF9f1" />
    <!-- A unique model Qm...., hello_aira model by default -->
    <param name="model" value="QmYb81uDNDHCnu9EZtYV4eoBDKRBAwJeNy1LT3p5Zbc357" />
    <!-- A token your CPS works with, bridged XRT token by default -->
    <param name="token" value="0xC02aaA39b223FE8D0A0e5C4F27eAD9083C756Cc2" />
    <!-- Ethereum blocks lifetime, ~12-15 sec for one -->
    <param name="order_lifetime" value="100" />
  </node>
</launch>
